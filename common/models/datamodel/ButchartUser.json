{
	"name": "ButchartUser",
	"base": "User",
	"strict": true,
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"properties": {
		"_id": {
			"type": "string",
			"id": true,
			"required": false
		},
		"tel": {
			"type": "string",
			"required": true
		},
		"email": {
			"type": "string",
			"required": false
		},
		"fullname": {
			"type": "string",
			"required": false
		},
		"password": {
			"type": "string",
			"required": false
		},
		"avatar": {
			"type": "string",
			"required": false
		},
		"birthday": {
			"type": "date",
			"required": false
		},
		"lastLoginDate": {
			"type": "date",
			"required": false
		},
		"registerDate": {
			"type": "date",
			"required": false
		}
	},
	"validations": [],
	"relations": {
		"relatedProfile": {
			"type": "embedsOne",
			"model": "UserProfile",
			"property": "userProfile"
		},
		"florist": {
			"type": "hasOne",
			"model": "Florist",
			"foreignKey": "_id"
		},
		"userShoppingCart":{
			"type": "embedsMany",
			"model": "ShoppingCartItem",
			"property": "shoppingCart"
		}
	},
	"acls": [],
	"methods": {}
}